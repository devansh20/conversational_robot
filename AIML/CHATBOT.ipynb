    "kernel.learn(\"start.xml\")\n",
    "kernel.respond(\"load aiml b\")\n",
    "\n",
    "# Press CTRL-C to break this loop\n",
    "print(\" \")\n",
    "print(\"Welcome to chat bot\")\n",
    "print(\" \")\n",
    "while True:\n",
    " print(kernel.respond(input(\"Type to talk>>\")))"
